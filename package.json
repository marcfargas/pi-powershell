{
  "name": "@marcfargas/pi-powershell",
  "version": "1.0.0",
  "description": "PowerShell tool for pi agents - Windows system integration and background processes",
  "author": "Marc Fargas",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcfargas/pi-powershell.git"
  },
  "keywords": [
    "pi",
    "pi-package",
    "agent",
    "powershell",
    "windows",
    "background-processes",
    "system-integration"
  ],
  "files": [
    "src/**/*.ts",
    "skills/**/*.md",
    "README.md",
    "LICENSE"
  ],
  "pi": {
    "extensions": [
      "./src/index.ts"
    ],
    "skills": [
      "./skills"
    ]
  },
  "peerDependencies": {
    "@mariozechner/pi-coding-agent": ">=0.50.0"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ __tests__/"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@mariozechner/pi-coding-agent": ">=0.50.0",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.4"
  }
}
